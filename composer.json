{
    "name": "z1lab/openid-client",
    "description": "OpenID Connect client for Laravel based on Passport.",
    "keywords": [
        "laravel",
        "passport",
        "openid",
        "client"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Felipe Pinheiro",
            "email": "filipe-pinheiro@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/auth": "~5.6",
        "illuminate/cookie": "^5.7",
        "illuminate/support": "~5.6",
        "lcobucci/jwt": "^3.2.2",
        "olimarferraz/laravel-meta-tags": "^3.1",
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "OpenID\\Client\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenID\\Client\\ClientServiceProvider"
            ],
            "aliases": {
                "OpenID": "OpenID\\Client\\Facades\\OpenidFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
