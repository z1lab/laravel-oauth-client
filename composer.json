{
    "name": "z1lab/openid-client",
    "description": "Z1lab OpenID Client is a package developed for connect Z1lab's Laravel projects to our Oauth2 Server.",
    "keywords": [
        "laravel",
        "passport",
        "openid",
        "client"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Felipe Pinheiro",
            "email": "filipe-pinheiro@hotmail.com"
        },
        {
            "name": "Olimar Ferraz",
            "email": "webmaster@z1ab.com.br"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/auth": "^5.7",
        "illuminate/cookie": "^5.7",
        "illuminate/support": "^5.7",
        "illuminate/view": "^5.7",
        "lcobucci/jwt": "^3.2.2",
        "z1lab/laravel-meta-tags": "^3.1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Z1lab\\OpenID\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Z1lab\\OpenID\\Providers\\OpenIDServiceProvider"
            ],
            "aliases": {
                "OpenID": "Z1lab\\OpenID\\Facades\\OpenidFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
